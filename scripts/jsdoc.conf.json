{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": [
      "./src/"
    ],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "lib/"
  },
  "plugins": [
    "./node_modules/jsdoc/plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "applicationName": "RGUI",
    "footer": "Made with ♥ by shitake",
    "copyright": "RGUI Copyright © 2017-2018 Mat shitake.",
    "disqus": "",
    "googleAnalytics": "",
    "openGraph": {
      "title": "",
      "type": "website",
      "image": "",
      "site_name": "",
      "url": ""
    },
    "meta": {
      "title": "",
      "description": "",
      "keyword": ""
    },
    "linenums": true
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  },
  "opts": {
    "encoding": "utf8",
    "recurse": true,
    "private": false,
    "lenient": true,
    "destination": "./docs",
    "template": "./node_modules/@pixi/jsdoc-template"
  }
}